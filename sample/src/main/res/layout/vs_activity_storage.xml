            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/vs_text_title_external_dir"
            android:textColor="@color/vs_red_primary"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_internal_dir_cache_content" />

        <TextView
            android:id="@+id/tv_external_dir_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textColor="@color/vs_blue_primary"
            android:textIsSelectable="true"
            app:layout_constraintStart_toStartOf="parent"
