            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_external_dir_private_cache_title"
            tools:text="XXXX" />

        <TextView
            android:id="@+id/tv_external_dir_public_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/vs_text_title_external_dir_public"
            android:textColor="@color/vs_red_primary"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_external_dir_private_cache_content" />

        <TextView
            android:id="@+id/tv_external_dir_public_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textColor="@color/vs_blue_primary"
            android:textIsSelectable="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_external_dir_public_title"
            tools:text="XXXX" />

        <TextView
            android:id="@+id/tv_internal_total_size_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/vs_text_title_internal_total_size"
            android:textColor="@color/vs_red_primary"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_external_dir_public_content" />

        <TextView
            android:id="@+id/tv_internal_total_size_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textColor="@color/vs_blue_primary"
            android:textIsSelectable="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_internal_total_size_title"
            tools:text="XXXX" />

        <TextView
